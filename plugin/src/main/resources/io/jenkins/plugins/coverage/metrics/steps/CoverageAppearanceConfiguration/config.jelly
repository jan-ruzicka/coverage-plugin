<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
  <f:section title="${%Coverage Plugin}">
    <f:entry title="${%title.defaultVisibility}" field="enableColumnByDefault">
      <f:checkbox/>
    </f:entry>

    <f:entry title="${%title.defaultColumnName}" field="defaultName">
      <f:textbox default="${%Coverage}"/>
    </f:entry>

    <f:entry title="${%title.defaultMetric}" field="defaultMetric">
      <f:select/>
    </f:entry>
  </f:section>
</j:jelly>
